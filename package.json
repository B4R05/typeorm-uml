{
	"name": "typeorm-uml",
	"version": "1.2.1",
	"description": "Generates UML diagrams for TypeORM projects",
	"author": "Eugene Manuilov <eugene.manuilov@gmail.com>",
	"license": "MIT",
	"homepage": "https://github.com/eugene-manuilov/typeorm-uml#readme",
	"bugs": {
		"url": "https://github.com/eugene-manuilov/typeorm-uml/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/eugene-manuilov/typeorm-uml.git"
	},
	"keywords": [
		"typeorm",
		"schema",
		"diagram",
		"uml",
		"database"
	],
	"engines": {
		"node": ">=8.0.0"
	},
	"files": [
		"/bin",
		"/lib"
	],
	"main": "lib/TypeormUmlCommand.js",
	"bin": {
		"typeorm-uml": "./bin/run"
	},
	"oclif": {
		"bin": "typeorm-uml"
	},
	"types": "lib/TypeormUmlCommand.d.ts",
	"scripts": {
		"prebuild": "rimraf lib",
		"build": "tsc -b --force",
		"watch": "tsc --watch",
		"prepublish": "npm run build",
		"test": "echo \"Error: no test specified\" && exit 1",
		"typeorm-uml": "./bin/run"
	},
	"dependencies": {
		"@oclif/command": "^1.8.0",
		"plantuml-encoder": "^1.4.0"
	},
	"devDependencies": {
		"@types/node": "^8.10.66",
		"rimraf": "^3.0.2",
		"typeorm": "^0.2.29",
		"typescript": "^3.9.7"
	},
	"peerDependencies": {
		"typeorm": "^0.2.0"
	}
}
